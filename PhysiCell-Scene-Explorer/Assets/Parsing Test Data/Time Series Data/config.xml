<PhysiCell_settings version="devel-version">
	<domain>
		<x_min>-500.0</x_min>
		<x_max>500.0</x_max>
		<y_min>-500.0</y_min>
		<y_max>500.0</y_max>
		<z_min>-10.0</z_min>
		<z_max>10.0</z_max>
		<dx>2.0</dx>
		<dy>2.0</dy>
		<dz>2.0</dz>
		<use_2D>true</use_2D>
	</domain>
	
	<overall>
		<max_time units="min">800.0</max_time>
		<time_units>min</time_units>
		<space_units>micron</space_units>
	</overall>
	
	<parallel>
		<omp_num_threads>4</omp_num_threads>
	</parallel> 
	
	<save>
		<folder>.</folder>  

		<full_data>
			<interval units="min">30</interval>
			<enable>True</enable>
		</full_data>
		
		<SVG>
			<interval units="min">30</interval>
			<enable>True</enable>
		</SVG>
		
		<legacy_data>
			<enable>false</enable>
		</legacy_data>
	</save>
	
	<microenvironment_setup>
		<variable ID="0" name="tracer" units="mol">
			<physical_parameter_set>
				<diffusion_coefficient units="micron^2/min">0.0</diffusion_coefficient>
				<decay_rate units="1/min">0.0</decay_rate>  
			</physical_parameter_set>
			<initial_condition units="mmHg">0.0</initial_condition>
			<Dirichlet_boundary_condition enabled="false" units="mmHg">0.0</Dirichlet_boundary_condition>
		</variable>
		
		<options>
			<calculate_gradients>False</calculate_gradients>
			<track_internalized_substrates_in_each_agent>False</track_internalized_substrates_in_each_agent>
		</options>
	</microenvironment_setup>	
	
	<user_parameters>
		<random_seed description="random seed" hidden="true" type="int" units="dimensionless">0</random_seed> 
		 
		<number_of_cells description="number of cell tracks to simulate" type="int" units="none">1000</number_of_cells>
		<is_motile description="true if cells are motile" type="bool" units="none">True</is_motile>
		<persistence_time description="mean persistence time" type="double" units="min">30.0</persistence_time>
		<migration_speed description="migration speed" type="double" units="micron/min">2.0</migration_speed> 
		<migration_bias description="migration bias parameter" type="double" units="none">0.8</migration_bias>
		<bias_migration_angle description="migration bias angle respect to x-axis" type="double" units="degree">30.0</bias_migration_angle>
		<fixed_persistence description="true if each migration path has same duration (future PhysiCell versions)" hidden="true" type="bool" units="none">False</fixed_persistence>
		<tracer_secretion_rate_constant description="number of cell tracks to simulate" hidden="true" type="double" units="1/min">1</tracer_secretion_rate_constant>
	</user_parameters>
	
</PhysiCell_settings>